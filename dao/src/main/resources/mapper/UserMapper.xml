<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.earthchen.spring.boot.dao.UserDao">


    <delete id="deleteAll">
        DELETE FROM user_info
        WHERE 1
    </delete>

    <select id="selectAll" resultType="com.earthchen.spring.boot.domain.User">
        select *
        from user
    </select>
</mapper>